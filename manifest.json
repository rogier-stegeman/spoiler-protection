{
  "manifest_version": 3,
  "name": "Spoiler protection",
  "description": "Hide potential spoilers.",
  "version": "1.2.0",
  "icons": {},
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/spoilerProtection.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"]
}
